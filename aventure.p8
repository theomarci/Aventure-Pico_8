-- badguys --

function create_badguys()
 new_bg={
	x=flr(rnd(127)/8),
	y=flr(rnd(127)/8),
	sprite=10,
	move=0
	}
add(badguys,new_bg)
end


function draw_badguys()
 if p.tomate==0 then 
   del(badguys,new_bg)
 else
		for bg in all(badguys) do
			spr(bg.sprite,bg.x*8,bg.y*8)
		end	
	end 
end


function badguys_mvt()
	local mbg={}
		mbg[1]={x=1,y=0}
		mbg[2]={x=-1,y=0}
		mbg[3]={x=0,y=-1}
		mbg[4]={x=0,y=1}
		move=mbg[flr(rnd(4))+1]
		for bg in all(badguys) do
		newx=bg.x
		newy=bg.y
			if check_flag(0,newx,newy) then
				del(badguys,bg)
			 create_badguys()
			end
				if(bg.move == 0) then
					bg.x=mid(0,newx,15)+move.x
					bg.y=mid(0,newy,15)+move.y
					
					if p.tomate<5 then
						bg.move=30
					elseif p.tomate<10 then
						bg.move=20
   		else
					bg.move=15
   		end
					
   else
    	bg.move-=1
   end
  end
end --function


function collision_bg(p,badguys)
	return not (p.x*8>=badguys.x*8+8
		or p.y*8>=badguys.y*8+8
		or p.x*8+8<=badguys.x*8
		or p.y*8+8<=badguys.y*8)
end
 
	

