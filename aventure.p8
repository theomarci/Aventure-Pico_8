-- tomate --

function create_tomate()
	t={
	x=flr(rnd(127)/8),
	y=flr(rnd(127)/8),
	sprite=6
	}
end


function draw_tomate()
	spr(t.sprite,t.x*8,t.y*8)	
end


function tomate_mvt()
	newx=t.x
	newy=t.y
		if check_flag(0,newx,newy) then
			del(t)
			create_tomate()
		else
				t.x=mid(0,newx,15)
				t.y=mid(0,newy,15)
	end
end


function pick_up_tomate()
		if collision_tomate(p,t) then
		  p.tomate+=1
				create_badguys()
				del(t)
			 create_tomate()
			 draw_tomate()
			 if p.tomate==10 then
			 	sfx(3)
			 	elseif p.tomate==20 then
			 	sfx(3)
			 	elseif p.tomate==30 then
			 	sfx(3)
			 	else sfx(2)
				end
		end
end


function collision_tomate(p,t)
	return not (p.x*8>=t.x*8+8
		or p.y*8>=t.y*8+8
		or p.x*8+8<=t.x*8
		or p.y*8+8<=t.y*8)
end

