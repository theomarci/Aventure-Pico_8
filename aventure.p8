-- ui --

function draw_ui()
	camera()
	palt(0,false)
	palt(12,true)
	if p.life==3 then
   spr(39,99,2)
   spr(39,107,2)
   spr(39,115,2)
 elseif p.life==2 then
  	spr(39,99,2)
   spr(39,107,2)
 else 
		 spr(39,99,2)
 end
 spr(8,2,2)
 palt()
 print_outline("X"..p.tomate,14,2)
end


function print_outline(text,x,y)
		print(text,x+1,y,0)
		print(text,x-1,y,0)
		print(text,x,y+1,0)
		print(text,x,y-1,0)
		print(text,x,y,7)
end


