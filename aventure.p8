-- player --

function create_player()
	p={
	x=2,y=2,
	ox=0,oy=0,
	start_ox=0,start_oy=0,
	anim_t=0,
	sprite=5,
	tomate=0,
	life=3
	}
end


function draw_player()
spr(p.sprite,
p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)
end


function player_movement()
newx=p.x
newy=p.y

	if p.anim_t==0 then
		if btn(➡️) then
	 newx+=1
	 newox=-8
	 newoy=0
	 p.flip=false
		elseif btn(⬅️) then
	 newx-=1
	 newox=8
	 newoy=0
	 p.flip=true
		elseif btn(⬇️) then
	 newy+=1
	 newox=0
	 newoy=-8
		elseif btn(⬆️) then
	 newy-=1
	 newox=0
	 newoy=8
		end
	end

	pick_up_tomate()
	player_die()
	
 if not check_flag(0,newx,newy)
 and (p.x!=newx or p.y!=newy) then
  p.x=mid(0,newx,15)
  p.y=mid(0,newy,15)
  p.start_ox=newox
  p.start_oy=newoy
  p.anim_t=1
	end
	
--animation player
	p.anim_t=max(p.anim_t-0.125,0)
	p.ox=p.start_ox*p.anim_t
	p.oy=p.start_oy*p.anim_t
	
	if p.anim_t>=0.75 then
		p.sprite=14
	elseif p.anim_t>=0.5 then
		p.sprite=13
	elseif p.anim_t>=0.25 then
		p.sprite=12
	else	p.sprite=11
	end

end --end function player mvt
   

function player_die()
 for bg in all(badguys) do
		if collision_bg(p,bg) then
					sfx(4)
					del(badguys,bg)
					create_badguys()
				 p.life-=1
					if p.life==0 then
						state=2
					end
 	end
	end
end


