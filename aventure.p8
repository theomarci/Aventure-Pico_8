-- start and end game --

function update_begin_game()
	if (btn(❎)) then
	state=1
	end
end


function draw_begin_game()
	cls(14)
	palt(0,false)
	palt(12,true)
	spr(136,0,8,3,4)
	spr(136,24,8,3,4)
	spr(136,48,8,3,4)
	spr(136,72,8,3,4)
	spr(136,96,8,3,4)
	spr(136,120,8,3,4)
	--
	spr(136,0,90,3,4)
	spr(136,24,90,3,4)
	spr(136,48,90,3,4)
	spr(136,72,90,3,4)
	spr(136,96,90,3,4)
	spr(136,120,90,3,4)
	palt()
	rectfill(8,53,126,79,0)
	rectfill(5,50,123,76,1)
	print("faites le plein de tomates ! ",10,56,10)
	print("press ❎ pour play ♥",20,66,9)
end


function update_end_game()		
	if (btn(❎)) _init()
end


function draw_end_game()			
	cls(14)
	
	palt(0,false)
	palt(12,true)
	spr(136,0,8,3,4)
	spr(136,24,8,3,4)
	spr(136,48,8,3,4)
	spr(136,72,8,3,4)
	spr(136,96,8,3,4)
	spr(136,120,8,3,4)
	--
	spr(136,0,90,3,4)
	spr(136,24,90,3,4)
	spr(136,48,90,3,4)
	spr(136,72,90,3,4)
	spr(136,96,90,3,4)
	spr(136,120,90,3,4)
	palt()
	
	rectfill(22,53,109,79,0)
	spr(10,60,60)
	rectfill(19,50,106,76,1)
	print("score : "..p.tomate,44,56,10)
	print("press ❎ pour replay",24,66,9)
end
