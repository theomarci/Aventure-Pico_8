version 38
__lua__
-- gameplay --

function _init()
	create_player()
	create_tomate()
	badguys={}
	create_badguys()
	state=0 
end


function _update()
if (state==0) update_begin_game()
if (state==1) update_game()
if (state==2) update_end_game()
end

function _draw()
if (state==0)	draw_begin_game()
if (state==1) draw_game()
if (state==2) draw_end_game()
end


-- jeu --

function update_game()
	player_movement()
	tomate_mvt()
	badguys_mvt()
	update_camera()
end

function draw_game()
	cls()
	draw_map()
	draw_player()
	draw_tomate()
	draw_badguys()
	draw_ui()
end
